<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <context:annotation-config />

    <!-- <bean/> definitions here -->


    <bean class="com.nabajyoti.springcoretut.autowire.annotation.Address" name="address">
        <property name="street" value="dhaleswar" />
    </bean>

    <bean class="com.nabajyoti.springcoretut.autowire.annotation.Address" name="address2">
        <property name="street" value="ramnagar" />
    </bean>

    <bean class="com.nabajyoti.springcoretut.autowire.annotation.Student" name="student1">

    </bean>
</beans>